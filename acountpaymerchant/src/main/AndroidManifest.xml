<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pay.acount.com.acountpaymerchant">

    <application
        android:allowBackup="true"
        android:name="pay.acount.com.main.AccountPayApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MerchantMenuActivity" />
        <activity android:name=".NewTransactionActivity" />
        <activity android:name=".QrCodeTransactionActivity" />
    </application>

</manifest>